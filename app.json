{
    "expo": {
        "name": "my-react-native-project",
        "slug": "my-react-native-project",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "aora",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.truejyro.aora"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-av",
            "expo-asset",
            "expo-document-picker",
            "expo-image-picker"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
